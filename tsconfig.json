// prettier-ignore
{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "tests": [
        "tests/*"
      ],
      "@Atoms/*": [
        "src/Atoms/*"
      ],
      "@Config/*": [
        "src/Config/*"
      ],
      "@Molecules/*": [
        "src/Molecules/*"
      ],
      "@Organisms/*": [
        "src/Organisms/*"
      ],
      "@Pages/*": [
        "src/Pages/*"
      ],
      "@Redux/*": [
        "src/Redux/*"
      ],
      "@Templates/*": [
        "src/Templates/*"
      ],
      "@Types/*": [
        "src/Types/*"
      ],
      "@Tests/*": [
        "src/Tests/*"
      ],
      "@Routes/*": [
        "src/Routes/*"
      ],
      "@Utils/*": [
        "src/Utils/*"
      ],
      "@Assets/*": [
        "src/Assets/*"
      ],
      "@Helpers/*": [
        "src/Helpers/*"
      ],
      "@Learning/*":[
        "src/Learning/*"
      ],
      "@JsonProccessing/*":[
        "src/JsonProccessing/*"
      ]
    },
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
